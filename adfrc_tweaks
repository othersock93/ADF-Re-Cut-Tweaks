		{
			class ItemInfo: VestItem
			{
				containerClass = "Supply160";
				class HitpointsProtectionInfo
				{
					class Chest
					{
						HitpointName = "HitChest";
						armor = 0;
						PassThrough = 1;
					};
					class Diaphragm
					{
						HitpointName = "HitDiaphragm";
						armor = 0;
						PassThrough = 1;
					};
					class Abdomen
					{
						HitpointName = "HitAbdomen";
